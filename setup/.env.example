# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_application_id_here
GUILD_ID=your_test_guild_id_here

# Environment Settings
NODE_ENV=development
LOG_LEVEL=debug
LOG_FILE=combined.log

# Audio Settings
MAX_QUEUE_SIZE=50
EXTRACTION_TIMEOUT=30000
INACTIVITY_TIMEOUT=300000

# FFmpeg Process Monitoring (for unlimited length video support)
FFMPEG_ACTIVITY_CHECK_INTERVAL=10000
FFMPEG_INACTIVE_WARNING_THRESHOLD=30000
FFMPEG_INACTIVE_KILL_THRESHOLD=60000
ENABLE_UNLIMITED_LENGTH=true

# Test Settings
TEST_MODE=false
TEST_GUILD_ID=your_test_guild_id_here
